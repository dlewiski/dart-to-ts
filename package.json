{
  "name": "dart-to-ts-converter",
  "version": "1.0.0",
  "description": "Convert Dart code to modern TypeScript while eliminating technical debt",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "analyze": "tsx scripts/analyze.ts",
    "convert": "tsx scripts/convert.ts",
    "report": "tsx scripts/report.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "keywords": [
    "dart",
    "typescript",
    "converter",
    "migration",
    "tech-debt"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/bedrock-sdk": "^0.10.2",
    "@aws-sdk/client-bedrock-runtime": "^3.478.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "ora": "^8.0.1",
    "p-limit": "^5.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.0",
    "tsx": "^4.20.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.44.1",
    "vitest": "^3.2.4"
  }
}
