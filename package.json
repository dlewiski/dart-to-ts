{
  "name": "dart-to-ts-analyzer",
  "version": "1.0.0",
  "description": "Dart to TypeScript application analyzer",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "analyze": "pnpm typecheck && pnpm build && node dist/index.js",
    "test": "pnpm typecheck && pnpm build && node dist/test/claude-integration.test.js",
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"*.json\" \"*.md\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" \"*.json\" \"*.md\"",
    "check": "pnpm typecheck && pnpm lint && pnpm format:check",
    "fix": "pnpm lint:fix && pnpm format"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0"
  }
}
