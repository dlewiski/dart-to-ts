{
  "appPurpose": "A web dashboard application for tracking and displaying software release information and dependencies",
  "coreFeatures": [
    "Configure transport platform for browser",
    "Remove loading frame from DOM",
    "Determine development vs production environment",
    "Initialize ReleaseDashboard component",
    "View app deployment information",
    "Expand/collapse package details",
    "Select/deselect apps from dropdown",
    "Save current search as bookmark",
    "Enter bookmark name"
  ],
  "userWorkflows": [
    {
      "name": "User Interactions",
      "steps": [
        "Click accordion to expand/collapse",
        "Click checkboxes to select apps",
        "Click Save Search button",
        "Type in bookmark name field",
        "Click Save bookmark button"
      ]
    }
  ],
  "dataFlow": {
    "sources": [
      "Package dependency API",
      "FEWS service API"
    ],
    "transformations": [
      "GET requests for package dependencies"
    ],
    "destinations": [
      "API responses"
    ]
  },
  "stateManagement": {
    "pattern": "fetchDashboardDataMiddleware, fetchAppsForDeployMiddleware, applyBookmarkMiddleware, filterUrlMiddleware, urlStateSyncMiddleware, clearAllFiltersMiddleware",
    "stateShape": {
      "query": "nullable string for package filter query",
      "deploys": "map of WdeskDeploy to DeployState containing apps and metadata",
      "recentSearches": "list of recent search query strings",
      "bookmarks": "list of SearchBookmark objects with label and selections",
      "selectedDeploys": "list of currently selected WdeskDeploy values",
      "selectedDeploysApps": "list of app names from selected deployments",
      "selectedApps": "list of currently selected app names",
      "clearFiltersActionCounter": "nullable counter for clear filters actions",
      "viewMode": "ViewMode enum (cards or table)",
      "versionThreshold": "nullable string for minimum version filtering"
    },
    "keyActions": [
      "FetchDashboardDataAction",
      "FetchAppsForDeployAction",
      "FetchedAppsForDeployAction",
      "FailedFetchAppsForDeployAction",
      "QueryUpdatedAction",
      "AddRecentSearchAction",
      "AddBookmarkAction",
      "RemoveBookmarkAction",
      "SelectDeployAction",
      "SelectAppAction",
      "DeselectDeployAction",
      "DeselectAppAction",
      "SetSelectedDeploysAction",
      "SetSelectedAppsAction",
      "ClearAllFiltersAction",
      "UpdateClearFiltersCounterAction",
      "ApplyBookmarkAction",
      "SetViewModeAction",
      "SetVersionThresholdAction",
      "ClearVersionThresholdAction"
    ],
    "selectors": [
      "appSelector",
      "appNamesSelector",
      "appVersionSelector",
      "hasFetchingDeployErrorSelector",
      "appDependenciesSelector",
      "packageNamesSelector",
      "packagesSelector",
      "packageSelector",
      "packageVersionSelector",
      "packageDependenciesSelector",
      "selectedDeploysSelector",
      "appsForSelectedDeploysSelector",
      "selectedAppsSelector",
      "fetchAllAppsSelector"
    ]
  },
  "businessLogic": {
    "rules": [],
    "validations": [],
    "calculations": []
  },
  "dependencies": {
    "dart": [
      "over_react",
      "redux",
      "built_value",
      "built_collection",
      "fluri",
      "w_transport"
    ],
    "tsEquivalents": {
      "over_react": "react",
      "redux": "@reduxjs/toolkit",
      "built_value": "immer + TypeScript interfaces",
      "built_collection": "immutable",
      "collection": "lodash",
      "fluri": "url-parse",
      "meta": "TypeScript built-in",
      "w_transport": "axios",
      "build_runner": "vite",
      "build_web_compilers": "vite + TypeScript",
      "glob": "glob",
      "dependency_validator": "npm-check-updates"
    }
  }
}